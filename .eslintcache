te({\n        text: val,\n        tasks: this.state.tasks,\n        counter: this.state.counter,\n        open: this.state.open,\n    }); \n  }\n   handleck=(id)=>{\n \n    this.setState({\n      \n    tasks : this.state.tasks.filter(item=>item.key!=id),\n    text: this.state.text,\n    counter: this.state.counter,\n    open: this.state.open,\n    })\n    if (this.state.tasks.length<1)\n    this.setState({\n      tasks : this.state.tasks,\n      text: this.state.text,\n    counter: this.state.counter,\n    open: false,\n  })\n    \n  }\n  \n\n\n  render() {\n    return (\n      <div>\n         \n        <Container>\n        <h1>Contact</h1>\n          <NewTodo\n            clickme={()=>this.handelClick()}\n            change={(e) => this.handelChange(e.target.value)}\n            mytext={this.state.text}\n          />\n          {this.state.tasks.map((item) => (\n            <Todo text={item.name} cle={item.key} bg=\"black\" cl=\"white\"  clik={(e) => this.handleck(e.target.id)}/>\n          ))}\n        </Container>\n      </div>\n    );\n  }\n}\n\nexport default Contact;\n","/Users/bigmac/Desktop/todo-list/src/Layouts/Header/index.jsx",[],"/Users/bigmac/Desktop/todo-list/src/Layouts/Footer/index.jsx",[],{"ruleId":"58","severity":1,"message":"59","line":6,"column":8,"nodeType":"60","messageId":"61","endLine":6,"endColumn":15},{"ruleId":"58","severity":1,"message":"62","line":7,"column":8,"nodeType":"60","messageId":"61","endLine":7,"endColumn":12},{"ruleId":"58","severity":1,"message":"63","line":9,"column":8,"nodeType":"60","messageId":"61","endLine":9,"endColumn":12},{"ruleId":"58","severity":1,"message":"64","line":10,"column":8,"nodeType":"60","messageId":"61","endLine":10,"endColumn":15},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"58","severity":1,"message":"69","line":1,"column":8,"nodeType":"60","messageId":"61","endLine":1,"endColumn":12},{"ruleId":"58","severity":1,"message":"70","line":7,"column":3,"nodeType":"60","messa